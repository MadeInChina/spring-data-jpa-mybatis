<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" 
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.mario6.springdata.jpa.mybatis.repository.demo.repository.UserRepository">

    <select id="selectByAccount" resultType="com.mario6.springdata.jpa.mybatis.repository.demo.domain.User">
        select * from user where account = #{account}
    </select>
</mapper>